# 환경 변수 설정
NODE_ENV=development
PORT=8000

# 데이터베이스
MONGODB_URI=mongodb://localhost:27017/invest-platform
REDIS_URL=redis://localhost:6379

# JWT 인증
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=7d

# OpenAI API
OPENAI_API_KEY=your-openai-api-key-here

# 증권사 API Keys (실제 사용시 각 증권사에서 발급받아야 함)
NAVER_API_KEY=your-naver-api-key
TOSS_API_KEY=your-toss-api-key
KAKAO_API_KEY=your-kakao-api-key

# 외부 API
YAHOO_FINANCE_API_KEY=your-yahoo-finance-key
KRX_API_KEY=your-krx-api-key

# 소켓 설정
SOCKET_CORS_ORIGIN=http://localhost:3000,http://localhost:3001

# 로그 레벨
LOG_LEVEL=debug
